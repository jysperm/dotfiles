export GOPATH=/Users/jysperm/Golang
export ETCDCTL_API=3

# If you come from bash you might have to change your $PATH.
export PATH=$PATH:/usr/local/opt/go/libexec/bin:/Users/jysperm/Golang/bin:/usr/local/openresty/nginx/sbin

# Path to your oh-my-zsh installation.
export ZSH=/Users/jysperm/.oh-my-zsh

ZSH_THEME="robbyrussell"

plugins=(git osx docker golang zsh-autosuggestions)

source $ZSH/oh-my-zsh.sh

# User configuration
# You may need to manually set your language environment
export LANG=en_US.UTF-8
